# Copy this file to .env and fill in your values
# Google/Gemini API
GOOGLE_API_KEY=your-google-api-key-here
GEMINI_MODEL_ROUTER=gemini-2.5-flash
GEMINI_MODEL_MAIN=gemini-2.5-pro

# Prompts directory
PROMPTS_DIR=./prompts

# VM Internal API (replace with your VM's private IP)
VM_INTERNAL_BASE_URL=http://10.0.0.10:8080/internal
VM_INTERNAL_TOKEN=your-internal-api-token

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION_MEMORY=user_memory

# OCI Configuration
OCI_STREAM_ID=ocid1.stream.oc1...
OCI_RESULT_QUEUE_ID=ocid1.queue.oc1...

# Logging
LOG_LEVEL=INFO

