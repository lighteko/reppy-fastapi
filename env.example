# LLM Settings
LLM_PROVIDER=openai
LLM_MODEL=gpt-4-turbo-preview
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=4096
OPENAI_API_KEY=your-openai-api-key-here

# Embedding Settings
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSION=1536

# Qdrant Settings
QDRANT_BACKEND=qdrant
# For remote Qdrant Cloud: https://your-cluster.cloud.qdrant.io
# For local Qdrant: http://localhost:6333
QDRANT_URL=https://your-cluster.cloud.qdrant.io
QDRANT_API_KEY=your-qdrant-api-key-here
QDRANT_GRPC=false
QDRANT_EXERCISES_COLLECTION=exercises
QDRANT_MEMORY_COLLECTION=user_memory
QDRANT_VECTOR_SIZE=1536
QDRANT_DISTANCE=Cosine
QDRANT_SEARCH_K=5
QDRANT_USE_MMR=true
QDRANT_MMR_LAMBDA=0.5
QDRANT_USE_RERANK=false

# Express Server Settings
EXPRESS_BASE_URL=http://localhost:3000
EXPRESS_API_KEY=
EXPRESS_TIMEOUT=30
EXPRESS_MAX_RETRIES=3
EXPRESS_RETRY_BACKOFF=1.0

# Agent Executor Settings
AGENT_MAX_ITERATIONS=8
AGENT_PARSING_RETRIES=2
TOOL_TIMEOUT=30
LLM_TIMEOUT=60

# Prompt Settings
PROMPTS_DIRECTORY=prompts

# Plate Rounding & Locale
PLATE_ROUNDING_INCREMENT=2.5
DEFAULT_LOCALE=en-US

# Logging & Observability
LOG_LEVEL=INFO
ENABLE_LANGSMITH=false
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=reppy-rag

# AWS Settings (for Lambda)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

